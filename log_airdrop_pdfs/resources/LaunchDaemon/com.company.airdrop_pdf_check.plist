<plist version="1.0">
<dict>
	<key>Label</key>
	<string>com.company.airdrop_pdf_check</string>
	<key>ProgramArguments</key>
	<array>
		<string>sh</string>
		<string>-c</string>
		<string>/Library/Scripts/log_airdrop_pdfs.sh</string>
